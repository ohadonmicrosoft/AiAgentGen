{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --ext .ts,.tsx . --fix",
    "fix-code": "tsx scripts/fix-code.ts --fix",
    "check-code": "tsx scripts/fix-code.ts",
    "db:push": "drizzle-kit push",
    "db:generate": "tsx server/migrations/generate.ts",
    "db:migrate": "tsx -e \"import { runMigrations } from './server/migrations'; runMigrations().catch(console.error)\"",
    "db:status": "tsx -e \"import { isDatabaseUpToDate } from './server/migrations'; isDatabaseUpToDate().then(upToDate => { console.log(upToDate ? 'Database is up to date' : 'Database needs migration'); process.exit(upToDate ? 0 : 1); }).catch(console.error)\"",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=config/config/jest.integration.config.js",
    "test:e2e": "playwright test",
    "test:visual": "playwright test e2e/visual-regression.spec.ts",
    "test:all": "npm run test && npm run test:integration && npm run test:e2e",
    "serve:dist": "http-server ./dist -p 3000 --silent",
    "db:setup:test": "node scripts/setup-test-db.js",
    "db:migrate:test": "node scripts/migrate-test-db.js",
    "analyze": "vite build --mode analyze",
    "cleanup": "tsx scripts/cleanup-project.ts",
    "prepare": "husky",
    "update:deps": "npx npm-check-updates -u && npm install",
    "security:check": "npm audit && npx snyk test",
    "format": "prettier --write .",
    "fix:all": "npm run format && npx @biomejs/biome check --apply .",
    "maintain": "tsx scripts/maintain.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
